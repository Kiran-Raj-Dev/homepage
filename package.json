{
    "name": "gatsby-starter-hello-world",
    "private": true,
    "description": "A simplified bare-bones starter for Gatsby",
    "version": "0.1.0",
    "license": "0BSD",
    "scripts": {
        "build": "gatsby build",
        "develop": "gatsby develop",
        "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
        "start": "gatsby develop",
        "serve": "gatsby serve",
        "clean": "gatsby clean",
        "type-check": "find ./src/ -name __generated__ -type d | xargs rm -r -v || true && node_modules/.bin/tsgql typegen && node_modules/.bin/tsc && echo 'type check ok'",
        "type-check:watch": "node_modules/.bin/tsc --watch",
        "load-gql": "node_modules/.bin/get-graphql-schema http://localhost:8000/___graphql > schema.graphql && yarn run type-check"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
    },
    "bugs": {
        "url": "https://github.com/gatsbyjs/gatsby/issues"
    },
    "devDependencies": {
        "@types/node": "^16.11.11",
        "@types/react": "^17.0.37",
        "@types/react-dom": "^17.0.11",
        "gatsby": "^4.3.0",
        "gatsby-link": "^4.3.0",
        "gatsby-plugin-image": "^2.3.0",
        "gatsby-plugin-sharp": "^4.3.0",
        "gatsby-plugin-tsconfig-paths": "^1.0.4",
        "gatsby-react-router-scroll": "^5.3.0",
        "gatsby-remark-images": "^6.3.0",
        "gatsby-source-filesystem": "^4.3.0",
        "gatsby-transformer-remark": "^5.3.0",
        "gatsby-transformer-sharp": "^4.3.0",
        "get-graphql-schema": "^2.1.2",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "ts-graphql-plugin": "^2.1.5",
        "typescript": "^4.5.2"
    }
}
